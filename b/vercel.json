{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel.py",
            "use": "@vercel/python"
        },
        {
            "src": "uploads/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/vercel.py"
        }
    ]
}